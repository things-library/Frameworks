<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFrameworks>net10.0</TargetFrameworks>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
		<IsPackable>true</IsPackable>
	</PropertyGroup>

	<PropertyGroup>
		<PackageId>ThingsLibrary.Entity.Local</PackageId>
		<PackageDescription>Things Library local (file) entity libraries and wrappers.</PackageDescription>
		<PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>

		<ProductName>ThingsLibrary.Entity.Local</ProductName>
		<Title>Local Entity Framework Library</Title>
		<Description>This is the shared library</Description>

		<Authors>Mark Lanning</Authors>
		<Company>Starlight Software Co.</Company>
	</PropertyGroup>
	
	<ItemGroup>
		<PackageReference Include="LiteDB" Version="5.0.17" />
	</ItemGroup>

	<ItemGroup>
	  <ProjectReference Include="..\ThingsLibrary.Entity\ThingsLibrary.Entity.csproj" />
	</ItemGroup>

</Project>
